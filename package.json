{
  "name": "nestjs-ttcache",
  "version": "1.0.0",
  "description": "Write-through cache implementation for NestJS with TypeORM inspired by Square's TTCache",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --config jest.integration.config.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "benchmark": "node benchmarks/run.js",
    "prepublishOnly": "npm run build && npm test",
    "prepare": "npm run build"
  },
  "dependencies": {
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.0.0",
    "@nestjs/core": "^11.0.0",
    "cache-manager": "^6.4.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.0"
  },
  "devDependencies": {
    "@nestjs/testing": "^11.1.8",
    "@types/jest": "^29.5.5",
    "@types/node": "^24.9.2",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "cache-manager-ioredis-yet": "^2.1.1",
    "eslint": "^8.51.0",
    "jest": "^29.7.0",
    "sqlite3": "^5.1.7",
    "ts-jest": "^29.1.1",
    "typescript": "^5.9.2"
  },
  "peerDependencies": {
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.0.0",
    "@nestjs/core": "^11.0.0",
    "cache-manager": "^6.4.3",
    "typeorm": "^0.3.0"
  },
  "private": false,
  "keywords": [
    "nestjs",
    "typeorm",
    "cache",
    "ttcache",
    "write-through",
    "redis",
    "typescript"
